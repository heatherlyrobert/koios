   yUNIT_mindnoc  ();

   strcpy (x_verb   , "test");
   x_spec    = KOIOS_SSCRP;
   x_conv    = CONV__scrp;
   x_code    = CODE__scrp;
   strcpy (x_stage  , "2b"  );
   x_vers    = '1';
   x_mark    = '5';
