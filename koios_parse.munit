char
koios_parse_field       (char a_scrp, char a_list, char a_unit)
{
   char       *x_name      = "apate.unit";
   char        x_desc      [LEN_LONG]  = "";
   char        x_meth      [LEN_HUND]  = "";
   char        x_args      [LEN_FULL]  = "";
   char        x_test      [LEN_LABEL] = "";
   char        x_expe      [LEN_RECD]  = "";
   char        x_retn      [LEN_FULL]  = "";
   char        x_which     [LEN_TITLE] = "";

   yUNIT_minscrpy ("koios", "koios_parse", "(PARSE) verify handling field content");
   if (a_list == '!')    { yUNIT_minprcs (); return 0; }
   if (a_unit != 0 && a_scrp != a_unit)  return 0;
   if (a_list == 'y')                    return 0;
   KOIOS_scrp_start ('y');

   yUNIT_mincond  ("verify a quick success");
   yerr_ureset   ();
   yUNIT_minval   ("set defaults"                       , PARSE__default  (x_desc, x_meth, x_args, x_test, x_expe, x_retn)                                ,    0);
   yUNIT_minval   ("call field handler"                 , PARSE__field    (x_name, 101, "PREP"   , KOIOS_STWO ,  2, "include the prototype headers"         , x_desc, x_meth, x_args, x_test, x_expe, x_retn, x_which),    1);
   yUNIT_minstr   ("... review desc"                    , x_desc              , "include the prototype headers");
   yUNIT_minstr   ("... review method"                  , x_meth              , "");
   yUNIT_minstr   ("... review arguments"               , x_args              , "");
   yUNIT_minstr   ("... review test"                    , x_test              , "");
   yUNIT_minstr   ("... review expected"                , x_expe              , "");
   yUNIT_minstr   ("... review return"                  , x_retn              , "");
   yUNIT_minstr   ("... review which"                   , x_which             , "");
   yUNIT_minval   ("... error count"                    , yerr_ulines (),    0);
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("attempt a non-existant spec");
   yerr_ureset   ();
   yUNIT_minval   ("set defaults"                       , PARSE__default  (x_desc, x_meth, x_args, x_test, x_expe, x_retn)                                ,    0);
   yUNIT_minval   ("call field handler"                 , PARSE__field    (x_name, 102, "PREP"   , '&'        ,  2, "include the prototype headers"         , x_desc, x_meth, x_args, x_test, x_expe, x_retn, x_which), -999);
   yUNIT_minval   ("... error count"                    , yerr_ulines (),    1);
   yUNIT_minstr   ("... check error message"            , yerr_upeek   (']'), "apate.unit:102:0: error: тPREPТ spec (&) col (2) not legal/defined");
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("attempt a null script AND non-existant spec");
   yerr_ureset   ();
   yUNIT_minval   ("set defaults"                       , PARSE__default  (x_desc, x_meth, x_args, x_test, x_expe, x_retn)                                ,    0);
   yUNIT_minval   ("call field handler (null scrp)"     , PARSE__field    (NULL  , 103, "PREP"   , '&'        ,  2, "include the prototype headers"         , x_desc, x_meth, x_args, x_test, x_expe, x_retn, x_which), -999);
   yUNIT_minval   ("... error count"                    , yerr_ulines (),    1);
   yUNIT_minstr   ("... check error message"            , yerr_upeek   (']'), "(n/a):103:0: error: тPREPТ spec (&) col (2) not legal/defined");
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("attempt a null verb AND non-existant spec");
   yerr_ureset   ();
   yUNIT_minval   ("set defaults"                       , PARSE__default  (x_desc, x_meth, x_args, x_test, x_expe, x_retn)                                ,    0);
   yUNIT_minval   ("call field handler (null scrp)"     , PARSE__field    (x_name, 104, NULL     , '&'        ,  2, "include the prototype headers"         , x_desc, x_meth, x_args, x_test, x_expe, x_retn, x_which), -999);
   yUNIT_minval   ("... error count"                    , yerr_ulines (),    1);
   yUNIT_minstr   ("... check error message"            , yerr_upeek   (']'), "apate.unit:104:0: error: т(n/a)Т spec (&) col (2) not legal/defined");
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("attempt a column too low");
   yerr_ureset   ();
   yUNIT_minval   ("set defaults"                       , PARSE__default  (x_desc, x_meth, x_args, x_test, x_expe, x_retn)                                ,    0);
