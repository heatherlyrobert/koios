   yUNIT_minval   ("clear wave file"                    , unlink (sm_nwave)  ,  666);
   yUNIT_minval   ("clear conv file"                    , unlink (sm_nconv)  ,  666);
   yUNIT_minval   ("clear old file"                     , unlink (sm_nold )  ,  666);
   yUNIT_minval   ("clear header"                       , unlink (sm_nhead)  ,  666);
