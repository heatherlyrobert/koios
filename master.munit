char
KOIOS__append             (char a_dir, char a_file [LEN_FULL], char a_recd [LEN_RECD])
{
   char        x_cmd       [LEN_RECD]  = "";
   static int  c           =    0;
   /*---(clear for first/rm)-------------*/
   if (a_dir == '[' || a_dir == '#') {
      c = 0;
      sprintf (x_cmd, "rm -f  %s  2> /dev/null", a_file);
      system (x_cmd);
   }
   if (a_dir == '#')  return 0;
   /*---(add record)---------------------*/
   strlcpy (x_cmd, "echo \"", LEN_RECD);
   strlcat (x_cmd, a_recd, LEN_RECD);
   if (a_dir == '[')  strlcat (x_cmd, " \"  >  ", LEN_RECD);
   else               strlcat (x_cmd, " \"  >> ", LEN_RECD);
   strlcat (x_cmd, a_file, LEN_RECD);
   ++c;
   /*  printf ("%4d  %3då%sæ\n", c, strlen (x_cmd), x_cmd);  */
   system (x_cmd);
   /*---(complete)-----------------------*/
   return 1;
}

