{
   char         x_name     [LEN_HUND] = "/tmp/apate.unit";
   char         t          [LEN_HUND] = "";
   FILE        *x_file     = NULL;
   char         x_cshare   =  '-';

   yUNIT_minscrpy ("koios", "koios_conv", "(CONV) verify step level verbs");
   PROG__unit_quiet  ();

   sprintf (t, "touch %s", x_name);  system (t);
   yUNIT_mincond  ("open the conversion file");
   yUNIT_minval   ("open file"                          , READ_open   (x_name, 'w', &x_file, NULL), 0);
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("verify writing an EXEC");
   yUNIT_minval   ("write conv line"                    , CONV_exec      (x_file    , "exec"    , "call the function"   , "DITTO_parse"    , "x, y, 21"       , "s_equal"   , "pretty expected", "x_str"     , '-', '-', ""  , &x_cshare  ),    0);
   yUNIT_minstr   ("... check result"                   , yURG_upeek     (x_name    ,   0)      , "     exec     call the function                    DITTO_parse                 x, y, 21                                                                                              s_equal     pretty expected                                                                                       x_str                ");
   yUNIT_minstr   ("... check result"                   , yURG_upeek     (x_name    ,   1)      , "(n/a)"                                    );
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("verify writing a LOAD");
   yUNIT_minval   ("write conv line"                    , CONV_load      (x_file    , "load"    , "prepare input"       , "stdin"          , ""               , ""          , "test  15  a"  , ""          , '-', '-', ""  , &x_cshare  ),    0);
   yUNIT_minstr   ("... check result"                   , yURG_upeek     (x_name    ,   0)      , "     exec     call the function                    DITTO_parse                 x, y, 21                                                                                              s_equal     pretty expected                                                                                       x_str                ");
   yUNIT_minstr   ("... check result"                   , yURG_upeek     (x_name    ,   1)      , "     load     prepare input                        stdin                       test  15  a                                                                                                                                                                                                              ");
   yUNIT_minstr   ("... check result"                   , yURG_upeek     (x_name    ,   2)      , "(n/a)"                                    );
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("verify writing a FILE");
