   yUNIT_minval   ("... error count"                    , yerr_ulines (),    1);
   yUNIT_minstr   ("... check error message"            , yerr_upeek   ('['), "apate.unit:25:0: error: verb тhandlerТ not recognized/found");
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("verify SCRP with stage data");
   yerr_ureset   ();
