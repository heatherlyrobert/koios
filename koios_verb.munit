   yUNIT_minchr   ("... check select"                   , x_select    , '-');
   yUNIT_minval   ("verb index"                         , VERB__index    (""    ), -999);
   yUNIT_mindnoc  ();

   yUNIT_mincond  ("verify another success");
   yerr_ureset   ();
