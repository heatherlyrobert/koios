   yUNIT_minstr   ("... check verb"                     , x_verb      , "");
   yUNIT_minval   ("... check index"                    , x_index     , -1);
   yUNIT_minchr   ("... check spec"                     , x_spec      , '-');
   yUNIT_minchr   ("... check location"                 , x_locn      , '-');
