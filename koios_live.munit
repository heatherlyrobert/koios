char
UNIT___live_reuse       (char a_scrp, char a_list, char a_unit)
{
   yUNIT_minscrpy ("koios", "koios_live", "(LIVE) verify condition numbers thru reuses");
   if (a_list == '!')    { yUNIT_minprcs (); return 0; }
   if (a_unit != 0 && a_scrp != a_unit)  return 0;
   if (a_list == 'y')                    return 0;
   KOIOS_prep_all ();

   KOIOS_reuse_master ();

   yUNIT_mincond ("call master conversion");
   yUNIT_minval  ("... run convert"                    , system ("koios --conv   master   > /dev/null  2>&1"),   0);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minstr  ("... line"                           , yerr_upeek  ('['     ), "(n/a)");
   yUNIT_mindnoc ();

   yUNIT_mincond ("call master encoding");
   yUNIT_minval  ("... run code"                       , system ("koios --code   master"), 999);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minstr  ("... line"                           , yerr_upeek  ('['     ), "(n/a)");
   yUNIT_mindnoc ();

   KOIOS_reuse_dataset ();

   yUNIT_mincond ("call dataset conversion");
   yUNIT_minval  ("... run convert"                    , system ("koios --conv   apate    > /dev/null  2>&1"),   0);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minstr  ("... line"                           , yerr_upeek  ('['     ), "(n/a)");
   yUNIT_mindnoc ();

   yUNIT_mincond ("call dataset encoding");
   yUNIT_minval  ("... run code"                       , system ("koios_debug --code   apate    > /dev/null  2>&1"),   0);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minstr  ("... line"                           , yerr_upeek  ('['     ), "(n/a)");
   yUNIT_mindnoc ();

   yerr_ureset    ();
   yUNIT_mincond ("turn into a unit test");
   yUNIT_minval  ("... copy to c"                      , system ("cp  -f /tmp/apate_unit.{cs,c}"), 0);
   yUNIT_minval  ("... compile"                        , system ("gcc -c /tmp/apate_unit.c   > /dev/null  2>&1"), 0);
   yUNIT_minval  ("... link"                           , system ("gcc -o /tmp/apate_unit /tmp/apate_unit.o -lyUNIT   > /dev/null  2>&1"), 0);
   yUNIT_minval  ("... permissions"                    , system ("chmod +x /tmp/apate_unit"), 0);
   yUNIT_mindnoc ();

   yUNIT_mincond ("check executing and presenting only conditions");
   yUNIT_minval  ("... run unit test"                  , system ("/tmp/apate_unit --conds"), 999);
   yUNIT_mindnoc ();

   KOIOS_reuse_output ();

   KOIOS_close_all ('*');
   yUNIT_minprcs  ();
   return 0;
}

char
main                    (int a_argc, char *a_argv [])
{
   /*---(locals)-----------+-----+-----+-*/
   int         x_unit      =    0;
   int         n           =    0;
   char        x_list      =    0;
   /*---(arguments)----------------------*/
   printf ("## %s\n", P_ONELINE);
   printf ("## pre-yUNIT unit testing : %s\n", a_argv [0]);
   if (a_argc >= 2) {
      if (strcmp (a_argv [1], "--list") == 0)       x_list = 'y';
      else if (strcmp (a_argv [1], "--inst") == 0)  x_list = '!';
      else                                          x_unit = atoi (a_argv [1]);
   }
   if (x_unit  >  0 )         printf ("## selected single script (%2d)\n", x_unit);
   else                       printf ("## selected all scripts\n");
   if (x_list == 'y')         printf ("## list script(s) only\n");
   else if (x_list == '!')    printf ("## generate initial wave file\n");
   else                       printf ("## execute conditions and steps\n");
   printf ("\n");
   /*---(startup)------------------------*/
   yUNIT_minunit ("koios_live");
   /*---(scripts)------------------------*/
   /* ························· ´·········································*/
   UNIT___live_master_conv      (++n, x_list, x_unit);
   UNIT___live_master_code      (++n, x_list, x_unit);
   UNIT___live_dataset_conv     (++n, x_list, x_unit);
   UNIT___live_dataset_code     (++n, x_list, x_unit);
   /*> koios_live_ditto            (++n, x_list, x_unit);                               <*/
   UNIT___live_scrp             (++n, x_list, x_unit);
   UNIT___live_cond             (++n, x_list, x_unit);
   UNIT___live_reuse            (++n, x_list, x_unit);
   UNIT___live_steps            (++n, x_list, x_unit);
   UNIT___live_step             (++n, x_list, x_unit);
   UNIT___live_full             (++n, x_list, x_unit);
   UNIT___live_stop             (++n, x_list, x_unit);
   /*> koios_live_vers             (++n, x_list, x_unit);                               <*/
   /*> koios_live_comment          (++n, x_list, x_unit);                               <*/
   /*> koios_live_code             (++n, x_list, x_unit);                               <*/
   /*---(wrapup)-------------------------*/
   yUNIT_mintinu    ();
   /*---(complete)-----------------------*/
   return 0;
}



