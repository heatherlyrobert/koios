char
koios_live_build        (void)
{
   KOIOS_master_create ();

   yUNIT_mincond ("call master conversion");
   yUNIT_minval  ("... run convert"                    , system ("koios --conv   master   > /dev/null  2>&1"),   0);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minstr  ("... line"                           , yerr_upeek  ('['     ), "(n/a)");
   yUNIT_minval  ("... run code"                       , system ("koios --code   master"), 999);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minstr  ("... line"                           , yerr_upeek  ('['     ), "(n/a)");
   yUNIT_mindnoc ();

   KOIOS_dataset_create ();

   yUNIT_mincond ("call dataset conversion");
   yUNIT_minval  ("... run convert"                    , system ("koios_debug --conv   apate    > /dev/null  2>&1"),   0);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minval  ("... run code"                       , system ("koios_debug --code   apate    > /dev/null  2>&1"),   0);
   yUNIT_minval  ("... error count"                    , yerr_ulines (),    0);
   yUNIT_minstr  ("... line"                           , yerr_upeek  ('['     ), "(n/a)");
   yUNIT_minval  ("... wave count"                     , yENV_ulines (sm_nwave),    8);
   yUNIT_mindnoc ();

   yerr_ureset    ();
   yUNIT_mincond ("turn into a unit test");
   yUNIT_minval  ("... copy to c"                      , system ("cp  -f /tmp/apate_unit.{cs,c}"), 0);
   yUNIT_minval  ("... compile"                        , system ("gcc -c /tmp/apate_unit.c   > /dev/null  2>&1"), 0);
   yUNIT_minval  ("... link"                           , system ("gcc -o /tmp/apate_unit /tmp/apate_unit.o -lyUNIT   > /dev/null  2>&1"), 0);
   yUNIT_minval  ("... permissions"                    , system ("chmod +x /tmp/apate_unit"), 0);
   yUNIT_minval  ("... wave count"                     , yENV_ulines (sm_nwave),    8);
   yUNIT_mindnoc ();

   return 0;
}

