#include    "master.munit"



char
UNIT___live_master_conv  (char a_scrp, char a_list, char a_unit)
{
   yUNIT_minscrpy ("koios", "koios_prog", "(LIVE) verify converting a serious master unit");
   if (a_list == '!')    { yUNIT_minprcs (); return 0; }
   if (a_unit != 0 && a_scrp != a_unit)  return 0;
   if (a_list == 'y')                    return 0;
   KOIOS_prep_all ();

   KOIOS_master_create ();

   yUNIT_mincond ("call master conversion");
   yUNIT_minval  ("... run convert"                    , system ("koios --conv   master   > /dev/null  2>&1"),   0);
   yUNIT_mindnoc ();

   KOIOS_master_verify  ();

   KOIOS_master_globals ();

   KOIOS_close_all ('*');
   yUNIT_minprcs  ();
   return 0;
}

