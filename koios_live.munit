   system ("echo \"  COND   (1)  repeat code                         \"                                                                                                       >> /tmp/apate.unit");
   system ("echo \"    exec      ... check integer                    yUNIT_testint     32                        i_equal     2                     \"                >> /tmp/apate.unit");
   system ("echo \"    exec      ... check character                  yUNIT_testchar    65                        i_equal     'A'                   \"                >> /tmp/apate.unit");
