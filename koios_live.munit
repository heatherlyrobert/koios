   yUNIT_minscrpy ("koios", "koios_live", "(LIVE) verify live ditto code run");
   if (a_list == '!')    { yUNIT_minprcs (); return 0; }
   if (a_unit != 0 && a_scrp != a_unit)  return 0;
   if (a_list == 'y')                    return 0;
   KOIOS_open_all ('*', '-');
   KOIOS_reset    ();

   yUNIT_mincond  ("create a sample unit with ditto");
